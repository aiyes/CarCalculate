package com.chinalife.sz.cc.domain;

import java.io.Serializable;
import java.util.Arrays;

/**
 * Created by tianwei on 2016/12/26.
 */
public class CombosForm implements Serializable {
    private static final long serialVersionUID = 3893516514713208695L;
    private String combosName;
    private ItemKindForm[] itemKindList;
    private String handlerName;
    private String handlerCode;

    @Override
    public String toString() {
        return "CombosForm{" +
                "combosName='" + combosName + '\'' +
                ", itemKindList=" + Arrays.toString(itemKindList) +
                ", handlerName='" + handlerName + '\'' +
                ", handlerCode='" + handlerCode + '\'' +
                '}';
    }

    public String getHandlerName() {
        return handlerName;
    }

    public void setHandlerName(String handlerName) {
        this.handlerName = handlerName;
    }

    public String getHandlerCode() {
        return handlerCode;
    }

    public void setHandlerCode(String handlerCode) {
        this.handlerCode = handlerCode;
    }

    public String getCombosName() {
        return combosName;
    }

    public void setCombosName(String combosName) {
        this.combosName = combosName;
    }

    public ItemKindForm[] getItemKindList() {
        return itemKindList;
    }

    public void setItemKindList(ItemKindForm[] itemKindList) {
        this.itemKindList = itemKindList;
    }
}
