<script>
    var app = angular.module('myApp', ['ui.bootstrap', 'ngCookies']);
    app.controller('myController', function ($scope, $modal, $http, $log) {
        var userCode = $.cookie('userCode');
        $scope.autoRefreshCombos=function(userCode){
            $http.post('combos/getCombos', JSON.stringify(userCode))
                    .success(function (data, status) {
                        $scope.combosSchemas = data;

                    })
                    .error(function (data, status) {
                        swal("系统提示", "获取商业险套餐信息失败", "error");
                    })
        }
        $scope.autoRefreshCombos(userCode);

//        修改套餐
        $scope.updateCombos = function (combos) {
            $scope.selectedCombos = combos;
            var modalInstance = $modal.open({
                templateUrl: 'addCombos.html',  //指向上面创建的视图
                controller: 'UpdateCombosCtrl',// 初始化模态范围
                size: 16,//大小配置
                resolve: {
                    selectedCombos: function () {
                        return $scope.selectedCombos;
                    }
                }
            });

            modalInstance.result.then(function (selectedItem) {
                selectedItem.handlerName = $.cookie('userName');
                selectedItem.handlerCode = $.cookie('userCode');
                $http.post("combos/updateCombos", selectedItem)
                        .success(function (data, status) {
                            $scope.autoRefreshCombos(userCode);
                        })
                        .error(function (data, status) {
                           swal("系统提示","更新商业险套餐失败","error");
                        })
            }, function () {

            });

        }
        //        新增套餐
        $scope.addCombos = function () {
           if($scope.combosSchemas.length>=3)
           {
               swal("系统提示","商业险套餐最多只允许配置三个","warning");
               return;
           }
            var modalInstance = $modal.open({
                templateUrl: 'addCombos.html',  //指向上面创建的视图
                controller: 'CombosCtrl',// 初始化模态范围
                size: 16 //大小配置

            });
            modalInstance.result.then(function (selectedItem) {
//                $scope.selecteditemKindList = selectedItem;

                selectedItem.handlerName = $.cookie('userName');
                selectedItem.handlerCode = $.cookie('userCode');
//                console.log(selectedItem);
//                return;
                $http.post("combos/saveCombos", selectedItem)
                        .success(function (data, status) {
                            $scope.autoRefreshCombos(userCode);
                        })
                        .error(function (data, status) {
                           swal("系统提示","商业险套餐添加失败","error");
                        })

            }, function () {
//                $scope.carDeviceList = angular.fromJson(initialCarDeviceList);
                $log.info('Modal dismissed at: ' + new Date());
            });

        }

//        删除套餐
        $scope.deleteCombos=function(combos){


            swal({
                title: '系统提示',
                text: "确定删除!",
                type: 'warning',
                showCancelButton: true,
                confirmButtonClass: "btn-danger",
                confirmButtonText: '确定',
                cancelButtonText: '取消',
                cancelButtonClass: 'btn btn-danger',
                closeOnConfirm: true
            }).then(function () {
                var url="combos/deleteCombos/"+userCode+"/"+combos.id;
                $http.delete(url, '')
                        .success(function(data, status){
                            $scope.autoRefreshCombos(userCode);
                        })
                        .error(function(data, status){
                            swal("系统提示","删除商业险套餐失败","error");
                        });
            }, function (dismiss) {

            })




        }
    });

    //    修改险种控制器
    angular.module('myApp').controller('UpdateCombosCtrl', function ($scope, $modalInstance, selectedCombos) { //依赖于modalInstance
        $scope.selectedCombos= selectedCombos;
         $scope.itemKindList=eval("("+ $scope.selectedCombos.itemKindList+")");
//        $scope.itemKindList=$scope.selectedCombos.itemkIndList;

        $scope.combos = {};
        $scope.combos.combosName=$scope.selectedCombos.combosName;
        $scope.combos.id=$scope.selectedCombos.id;
        $scope.kind=[];
        //基本险种
        $scope.temp = [
            {
                "itemKindList": [
                    {"kindCode": "001", "kindName": "机动车损失保险", "deductableFlag": "0"},
                    {"kindCode": "002", "kindName": "机动车第三者责任保险", "deductableFlag": "0"},
                    {"kindCode": "007", "kindName": "机动车全车盗抢保险", "deductableFlag": "0"},
                    {"kindCode": "003", "kindName": "机动车车上人员责任保险（司机）", "deductableFlag": "0"},
                    {"kindCode": "006", "kindName": "机动车车上人员责任保险（乘客）", "deductableFlag": "0"},
                    {"kindCode": "205", "kindName": "机动车划痕损失险", "deductableFlag": "0"},
                    {"kindCode": "201", "kindName": "玻璃单独破碎险", "deductableFlag": "0"},
                    {"kindCode": "202", "kindName": "自燃损失险", "deductableFlag": "0"},
                    {"kindCode": "206", "kindName": "发动机涉水损失险", "deductableFlag": "0"},
                    {"kindCode": "210", "kindName": "机动车损失保险无法找到第三方特约险", "deductableFlag": "0"},
