package com.chinalife.sz.cc.domain;

import java.io.Serializable;
import java.util.Arrays;

/**
 * 历史数据复用接口返回Javabean
 * @author MarsCheng
 *
 */
public class PolicyDataReuseResponse implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 3840528892242601393L;
	//主信息
	private MainRequest mainEhm;
	//投保人
	private ApplicantRequest applicantEhm;
	//被保人信息
	private InsuredDataRequest[] insuredDataEhmArr;
	//车辆信息
	private CarInfoRequest carInfoEhm;
	//新增设备
	private CarDeviceDataRequest[] carDeviceDataEhmArr;
	//驾驶员列表
	private CarDriverDataRequest[] carDriverDataEhmArr;
	//车船税信息
	private CarShipTaxInfoRequest carShipTaxInfoEhm;
	//险种 信息
	private ItemKindDataRequest[] itemKindDataEhmArr;


	@Override
	public String toString() {
		return "PolicyDataReuseResponse{" +
				"mainEhm=" + mainEhm +
				", applicantEhm=" + applicantEhm +
				", insuredDataEhmArr=" + Arrays.toString(insuredDataEhmArr) +
				", carInfoEhm=" + carInfoEhm +
				", carDeviceDataEhmArr=" + Arrays.toString(carDeviceDataEhmArr) +
				", carDriverDataEhmArr=" + Arrays.toString(carDriverDataEhmArr) +
				", carShipTaxInfoEhm=" + carShipTaxInfoEhm +
				", itemKindDataEhmArr=" + Arrays.toString(itemKindDataEhmArr) +
				'}';
	}

	public MainRequest getMainEhm() {
		return mainEhm;
	}

	public void setMainEhm(MainRequest mainEhm) {
		this.mainEhm = mainEhm;
	}

	public ApplicantRequest getApplicantEhm() {
		return applicantEhm;
	}

	public void setApplicantEhm(ApplicantRequest applicantEhm) {
		this.applicantEhm = applicantEhm;
	}

	public InsuredDataRequest[] getInsuredDataEhmArr() {
		return insuredDataEhmArr;
	}

	public void setInsuredDataEhmArr(InsuredDataRequest[] insuredDataEhmArr) {
		this.insuredDataEhmArr = insuredDataEhmArr;
	}

	public CarInfoRequest getCarInfoEhm() {
		return carInfoEhm;
	}

	public void setCarInfoEhm(CarInfoRequest carInfoEhm) {
		this.carInfoEhm = carInfoEhm;
	}

	public CarDeviceDataRequest[] getCarDeviceDataEhmArr() {
		return carDeviceDataEhmArr;
	}

	public void setCarDeviceDataEhmArr(CarDeviceDataRequest[] carDeviceDataEhmArr) {
		this.carDeviceDataEhmArr = carDeviceDataEhmArr;
	}

	public CarDriverDataRequest[] getCarDriverDataEhmArr() {
		return carDriverDataEhmArr;
	}

	public void setCarDriverDataEhmArr(CarDriverDataRequest[] carDriverDataEhmArr) {
		this.carDriverDataEhmArr = carDriverDataEhmArr;
	}

	public CarShipTaxInfoRequest getCarShipTaxInfoEhm() {
		return carShipTaxInfoEhm;
	}

	public void setCarShipTaxInfoEhm(CarShipTaxInfoRequest carShipTaxInfoEhm) {
		this.carShipTaxInfoEhm = carShipTaxInfoEhm;
	}

	public ItemKindDataRequest[] getItemKindDataEhmArr() {
		return itemKindDataEhmArr;
	}

	public void setItemKindDataEhmArr(ItemKindDataRequest[] itemKindDataEhmArr) {
		this.itemKindDataEhmArr = itemKindDataEhmArr;
	}
}
